{
  "exclude_files": ["lib/", "test/"],
  "exclude_informational": true,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "detectors_to_exclude": ["solc-version", "pragma", "naming-convention"],
  "filter_paths": ["test", "example"],
  "triage_mode": false,
  "fail_none": false,
  "fail_high": true,
  "fail_medium": true,
  "fail_low": false,
  "solc_remaps": [
    "@uniswap/v2-core/=lib/v2-core/",
    "@uniswap/lib/=lib/lib/",
    "@openzeppelin/=lib/openzeppelin-contracts/"
  ]
}
